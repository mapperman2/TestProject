[user]
	name = Yurii Tykhonenko
	email = tihonenko.j@gmail.com
[credential]
	helper = manager
[core]
	editor = \"C:/Program Files (x86)/notepad++/notepad++.exe\" -multiInst -nosession
[pull]
	rebase = false
[fetch]
	prune = false
[rebase]
	autoStash = false
[diff]
	guitool = tortoisemerge
[difftool "kdiff3"]
	cmd = \"\" \"$LOCAL\" \"$REMOTE\"
[merge]
	guitool = tortoisemerge
[mergetool "kdiff3"]
	cmd = \"\" \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
[difftool "tortoisemerge"]
	path = C:/Program Files/TortoiseGit/bin/TortoiseGitMerge.exe
	cmd = \"C:/Program Files/TortoiseGit/bin/TortoiseGitMerge.exe\" \"$LOCAL\" \"$REMOTE\"
[mergetool "tortoisemerge"]
	path = C:/Program Files/TortoiseGit/bin/TortoiseGitMerge.exe
	cmd = \"C:/Program Files/TortoiseGit/bin/TortoiseGitMerge.exe\" -base:\"$BASE\" -mine:\"$LOCAL\" -theirs:\"$REMOTE\" -merged:\"$MERGED\"
